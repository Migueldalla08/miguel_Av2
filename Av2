//Luiza e José
let aluno=("Miguel");

var Mnota1, Mnota2, Mnota3, Mmedia, Mfrequencia, Lnota1, Lnota2, Lnota3, Lmedia, Lfrequencia, Jnota1, Jnota2, Jnota3, Jmedia, Lfrequencia;

Mnota1 = Number(prompt("Digite a primeira nota de Miguel"));
Mnota2 = Number(prompt("Digite a segunda nota de Miguel"));
Mnota3 = Number(prompt("Digite a terceira nota de Miguel"));
Mfrequencia = Number(prompt("Digite a frequencia de Miguel de 0 a 100"));

if (Mfrequencia >= 85) {
  console.log("Frequencia boa");
} else {
  if (Mfrequencia <= 84)
  console.log("Reprovado por frequencia");
}

Mmedia = (Mnota1+Mnota2+Mnota3)/3;

console.log("a media das notas de Miguel é: "+(Mmedia));

if ( Mmedia >= 6 ) {
   console.log("Aluno aprovado");
     } else {
     if ( Mmedia <= 5) {
       console.log("Aluno reprovado");
     }}

